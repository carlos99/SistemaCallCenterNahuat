<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/MyBundle</base-name>
            <var>myBundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>planillaBean</managed-bean-name>
        <managed-bean-class>com.isw2.nahuat.bean.planillaBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>usuarioBean</managed-bean-name>
        <managed-bean-class>com.isw2.nahuat.bean.UsuarioBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>edBean</managed-bean-name>
        <managed-bean-class>com.isw2.nahuat.bean.edBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>planillaBean1</managed-bean-name>
        <managed-bean-class>com.isw2.nahuat.bean.PlanillaBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <application>
        <message-bundle>error</message-bundle>
    </application>
</faces-config>
